{
    "name": "cloud-api",
    "version": "1.0.0",
    "author": "exomia",
    "license": "MIT",
    "scripts": {
        "start": "nodemon index.js --exec babel-node",
        "production": "babel-node index.js",
        "update": "npm prune && npm-check -u"
    },
    "nodemonConfig": {
        "ignore": [],
        "delay": "50",
        "watch": [
            "*.*"
        ],
        "ext": "js html json"
    },
    "dependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "cookie-parser": "^1.4.3",
        "cors": "^2.8.5",
        "express": "^4.16.4",
        "jsonwebtoken": "^8.4.0",
        "multer": "^1.4.1",
        "nodemailer": "^4.7.0",
        "pg": "^7.6.1"
    },
    "devDependencies": {
        "cross-env": "^5.2.0",
        "nodemon": "^1.18.6"
    }
}
